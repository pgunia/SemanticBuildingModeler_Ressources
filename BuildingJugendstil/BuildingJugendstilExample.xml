<?xml version="1.0" encoding="UTF-8"?>

<buildingJugendstil
    xmlns="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/BuildingJugendstil"
    xsi:schemaLocation="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/BuildingJugendstil 
    http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/BuildingJugendstil/BuildingJugendstilSchema.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:smcb_ct="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/CommonTypes"
    xmlns:smcb_eb="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/ExampleBasedSynthesis"
    xmlns:smcb_cb="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/ClassBasedFootprint"
    xmlns:smcb_ro="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/Roof"
    xmlns:smcb_bu="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/Building"
    xmlns:smcb_co="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/Component"
    xmlns:smcb_fp="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/Footprint"
    xmlns:smcb_op="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/Objectplacement"
    xmlns:smcb_pb="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/PolygonFootprint"
    xmlns:smcb_fl="http://crazyhorse.archaeologie.uni-koeln.de/~pgunia/SchemaDocuments/Floor">
    <smcb_bu:dimensions>
        <smcb_bu:height>
            <smcb_ct:lowerBorder>100</smcb_ct:lowerBorder>
            <smcb_ct:upperBorder>150</smcb_ct:upperBorder>
        </smcb_bu:height>
        <smcb_bu:length>
            <smcb_ct:lowerBorder>70</smcb_ct:lowerBorder>
            <smcb_ct:upperBorder>100</smcb_ct:upperBorder>
        </smcb_bu:length>
        <smcb_bu:width>
            <smcb_ct:lowerBorder>40</smcb_ct:lowerBorder>
            <smcb_ct:upperBorder>80</smcb_ct:upperBorder>
        </smcb_bu:width>
        <smcb_bu:wallThickness>5</smcb_bu:wallThickness>
    </smcb_bu:dimensions>
    <floorHeights>
        <floorHeight>
            <position>GROUND</position>
            <height>
                <smcb_ct:lowerBorder>30</smcb_ct:lowerBorder>
                <smcb_ct:upperBorder>45</smcb_ct:upperBorder>
            </height>
        </floorHeight>
        <floorHeight>
            <position>INTERMEDIATE</position>
            <height>
                <smcb_ct:lowerBorder>35</smcb_ct:lowerBorder>
                <smcb_ct:upperBorder>55</smcb_ct:upperBorder>
            </height>
        </floorHeight>
        <floorHeight>
            <position>TOP</position>
            <height>
                <smcb_ct:lowerBorder>30</smcb_ct:lowerBorder>
                <smcb_ct:upperBorder>45</smcb_ct:upperBorder>
            </height>
        </floorHeight>
    </floorHeights>
    <smcb_ro:roof>
        <smcb_ro:roofDescriptor>
            <smcb_ro:roofSlopeMain>
                <smcb_ro:lowerBorder>0.8</smcb_ro:lowerBorder>
                <smcb_ro:upperBorder>1.2</smcb_ro:upperBorder>
            </smcb_ro:roofSlopeMain>
            <smcb_ro:roofSlopeSide>
                <smcb_ro:lowerBorder>1.1</smcb_ro:lowerBorder>
                <smcb_ro:upperBorder>1.57</smcb_ro:upperBorder>
            </smcb_ro:roofSlopeSide>
            <smcb_ro:roofScaling>1.0</smcb_ro:roofScaling>
            <smcb_ro:equalWeightForRoofSides>0.0</smcb_ro:equalWeightForRoofSides>
        </smcb_ro:roofDescriptor>
    </smcb_ro:roof>
    <smcb_bu:buildingComponents>
        <smcb_co:component>
            <smcb_co:window>
                <smcb_co:componentModelSource>
                    <smcb_co:modelCategory>Window_Jugendstil</smcb_co:modelCategory>
                </smcb_co:componentModelSource>
                <smcb_co:translationRatioForWindows>0.4</smcb_co:translationRatioForWindows>
                <smcb_co:minDistanceBetweenWindowsRatio>0.05</smcb_co:minDistanceBetweenWindowsRatio>
                <smcb_co:windowToTargetQuadRatio>
                    <smcb_ct:lowerBorder>0.3</smcb_ct:lowerBorder>
                    <smcb_ct:upperBorder>0.5</smcb_ct:upperBorder>
                </smcb_co:windowToTargetQuadRatio>
            </smcb_co:window>
        </smcb_co:component>
        <smcb_co:component>
            <smcb_co:door>
                <smcb_co:componentModelSource>
                    <smcb_co:modelCategory>Door</smcb_co:modelCategory>
                </smcb_co:componentModelSource>
                <smcb_co:distanceToCornerRatio>0.1</smcb_co:distanceToCornerRatio>
                <smcb_co:doorToTargetQuadRatio>
                    <smcb_ct:lowerBorder>0.5</smcb_ct:lowerBorder>
                </smcb_co:doorToTargetQuadRatio>
            </smcb_co:door>
        </smcb_co:component>
        <smcb_co:component>
            <smcb_co:windowLedge>
                <smcb_co:componentModelSource>
                    <smcb_co:modelCategory>Moulding_Eckstein</smcb_co:modelCategory>
                </smcb_co:componentModelSource>
                <smcb_co:ledgeToTargetQuadRatio>0.1</smcb_co:ledgeToTargetQuadRatio>
            </smcb_co:windowLedge>
        </smcb_co:component>
        <smcb_co:component>
            <smcb_co:fascia>
                <smcb_co:componentModelSource>
                    <smcb_co:modelCategory>Moulding_Gotisch</smcb_co:modelCategory>
                </smcb_co:componentModelSource>
                <smcb_co:mouldingHeightRatio>0.1</smcb_co:mouldingHeightRatio>
                <smcb_co:targetFloors>
                    <smcb_co:floorPosition>ALL</smcb_co:floorPosition>
                </smcb_co:targetFloors>
            </smcb_co:fascia>
        </smcb_co:component>
    </smcb_bu:buildingComponents>
    <jugendstilProbabilities>
        <modifyGroundFootprintProbability>0.4</modifyGroundFootprintProbability>
        <modifyIntermediateFootprintProbability>0.4</modifyIntermediateFootprintProbability>
        <modifyTopFootprintProbability>0.2</modifyTopFootprintProbability>
        <useGroundFloorFootprintForTopFloorProbability>0.7</useGroundFloorFootprintForTopFloorProbability>
    </jugendstilProbabilities>
    <objectplacementConfiguration>
        <smcb_op:objectplacement>
            <smcb_op:maxCornerMainComponentDimensionsRatio>0.4</smcb_op:maxCornerMainComponentDimensionsRatio>
            <smcb_ct:polygonSource>
                <smcb_ct:modelCategory>Footprint</smcb_ct:modelCategory>
            </smcb_ct:polygonSource>
            <smcb_op:placementScalingSubcomponents>
                <smcb_ct:lowerBorder>0.6</smcb_ct:lowerBorder>
                <smcb_ct:upperBorder>0.9</smcb_ct:upperBorder>
            </smcb_op:placementScalingSubcomponents>
            <smcb_op:placementScalingMaincomponents>
                <smcb_ct:lowerBorder>0.4</smcb_ct:lowerBorder>
                <smcb_ct:upperBorder>0.7</smcb_ct:upperBorder>
            </smcb_op:placementScalingMaincomponents>

            <smcb_op:minNumberOfFreeBlocks>3</smcb_op:minNumberOfFreeBlocks>
            <smcb_op:maxNumberOfSubcomponentsPerRay>1</smcb_op:maxNumberOfSubcomponentsPerRay>
            <smcb_op:minSubcomponentSizeInBlocks>4</smcb_op:minSubcomponentSizeInBlocks>
            <smcb_op:allowedSubcomponentTypes>
                <smcb_op:type>RectComponent</smcb_op:type>
                <smcb_op:type>CylindricComponent</smcb_op:type>
            </smcb_op:allowedSubcomponentTypes>
            <smcb_op:maxNumberOfRetriesPerRay>10</smcb_op:maxNumberOfRetriesPerRay>

            <smcb_op:heightWidthRatioComponents>
                <smcb_ct:lowerBorder>0.3</smcb_ct:lowerBorder>
                <smcb_ct:upperBorder>0.95</smcb_ct:upperBorder>
            </smcb_op:heightWidthRatioComponents>
            <smcb_op:maxSubMainComponentWidthRatio>0.7</smcb_op:maxSubMainComponentWidthRatio>
            <smcb_op:minMainToSubcomponentDeviance>0.07</smcb_op:minMainToSubcomponentDeviance>
            <smcb_op:numberOfIterations>1</smcb_op:numberOfIterations>

            <smcb_op:placementProbability>
                <smcb_op:symmetry>0.2</smcb_op:symmetry>
                <smcb_op:positionSubComponentsOnCorners>0.4</smcb_op:positionSubComponentsOnCorners>
                <smcb_op:positionCylinders>0.2</smcb_op:positionCylinders>
                <smcb_op:positionComponentsOnEdge>0.6</smcb_op:positionComponentsOnEdge>
                <smcb_op:positionOnCenter>0.4</smcb_op:positionOnCenter>
                <smcb_op:convexHull>0.0</smcb_op:convexHull>
            </smcb_op:placementProbability>
        </smcb_op:objectplacement>
    </objectplacementConfiguration>
</buildingJugendstil>
