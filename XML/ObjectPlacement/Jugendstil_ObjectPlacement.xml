<?xml version="1.0" encoding="UTF-8"?>
<objectplacement
    xmlns="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Objectplacement"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sbm_ct="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/CommonTypes"
    xmlns:sbm_op="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Objectplacement"
    xsi:schemaLocation="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Objectplacement https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Objectplacement/ObjectplacementSchema.xsd">
        <sbm_op:maxCornerMainComponentDimensionsRatio>0.4</sbm_op:maxCornerMainComponentDimensionsRatio>
        <sbm_ct:polygonSource>
            <sbm_ct:modelCategory>Footprint</sbm_ct:modelCategory>
        </sbm_ct:polygonSource>
        <sbm_op:placementScalingSubcomponents>
            <sbm_ct:lowerBorder>0.6</sbm_ct:lowerBorder>
            <sbm_ct:upperBorder>0.9</sbm_ct:upperBorder>
        </sbm_op:placementScalingSubcomponents>
        <sbm_op:placementScalingMaincomponents>
            <sbm_ct:lowerBorder>0.4</sbm_ct:lowerBorder>
            <sbm_ct:upperBorder>0.7</sbm_ct:upperBorder>
        </sbm_op:placementScalingMaincomponents>
        
        <sbm_op:minNumberOfFreeBlocks>3</sbm_op:minNumberOfFreeBlocks>
        <sbm_op:maxNumberOfSubcomponentsPerRay>1</sbm_op:maxNumberOfSubcomponentsPerRay>
        <sbm_op:minSubcomponentSizeInBlocks>5</sbm_op:minSubcomponentSizeInBlocks>
        <sbm_op:allowedSubcomponentTypes>
            <sbm_op:type>RectComponent</sbm_op:type>
            <sbm_op:type>CylindricComponent</sbm_op:type>
        </sbm_op:allowedSubcomponentTypes>
        <sbm_op:maxNumberOfRetriesPerRay>10</sbm_op:maxNumberOfRetriesPerRay>
        
        <sbm_op:heightWidthRatioComponents>
            <sbm_ct:lowerBorder>0.3</sbm_ct:lowerBorder>
            <sbm_ct:upperBorder>0.95</sbm_ct:upperBorder>
        </sbm_op:heightWidthRatioComponents>
        <sbm_op:maxSubMainComponentWidthRatio>0.7</sbm_op:maxSubMainComponentWidthRatio>
        <sbm_op:minMainToSubcomponentDeviance>0.1</sbm_op:minMainToSubcomponentDeviance>
        <sbm_op:numberOfIterations>2</sbm_op:numberOfIterations>
        
        <sbm_op:placementProbability>
            <sbm_op:symmetry>0.2</sbm_op:symmetry>
            <sbm_op:positionSubComponentsOnCorners>0.4</sbm_op:positionSubComponentsOnCorners>
            <sbm_op:positionCylinders>0.2</sbm_op:positionCylinders>
            <sbm_op:positionComponentsOnEdge>0.6</sbm_op:positionComponentsOnEdge>
            <sbm_op:positionOnCenter>0.4</sbm_op:positionOnCenter>
            <sbm_op:convexHull>0.3</sbm_op:convexHull>
        </sbm_op:placementProbability>
</objectplacement>
