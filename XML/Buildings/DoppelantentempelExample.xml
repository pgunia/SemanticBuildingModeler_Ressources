<?xml version="1.0" encoding="UTF-8"?>

<buildingDoppelantentempel 
 xmlns:sbm_ct="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/CommonTypes"
 xmlns:sbm_eb="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/ExampleBasedSynthesis"
 xmlns:sbm_cb="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/ClassBasedFootprint"
 xmlns="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/BuildingDoppelantentempel"
 xmlns:sbm_ro="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Roof"
 xmlns:sbm_bu="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Building"
 xmlns:sbm_co="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Component"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:sbm_fp="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Footprint"
 xmlns:sbm_op="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Objectplacement"
 xmlns:sbm_pb="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/PolygonFootprint"
 xmlns:sbm_fl="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Floor"
 xsi:schemaLocation="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/BuildingDoppelantentempel https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/BuildingDoppelantentempel/BuildingDoppelantentempelSchema.xsd">
    <sbm_bu:dimensions>
        <sbm_bu:height>
            <sbm_ct:lowerBorder>150</sbm_ct:lowerBorder>
        </sbm_bu:height>
        <sbm_bu:length>
            <sbm_ct:lowerBorder>1000</sbm_ct:lowerBorder>
        </sbm_bu:length>
        <sbm_bu:width>
            <sbm_ct:lowerBorder>700</sbm_ct:lowerBorder>
        </sbm_bu:width>
        <sbm_bu:wallThickness>5</sbm_bu:wallThickness>
    </sbm_bu:dimensions>
    <doppelantentempelParameters>
        <architraveHeight>20</architraveHeight>
        <metopeHeight>15</metopeHeight>
        <geisonHeight>15</geisonHeight>
    </doppelantentempelParameters>
    <sbm_ro:roof>
        <sbm_ro:roofDescriptor>
            <sbm_ro:roofSlopeMain>
                <sbm_ro:lowerBorder>0.25</sbm_ro:lowerBorder>
            </sbm_ro:roofSlopeMain>
            <sbm_ro:roofSlopeSide>
                <sbm_ro:lowerBorder>1.56</sbm_ro:lowerBorder>
            </sbm_ro:roofSlopeSide>
            <sbm_ro:roofScaling>1.0</sbm_ro:roofScaling>
            <sbm_ro:equalWeightForRoofSides>0.0</sbm_ro:equalWeightForRoofSides>
        </sbm_ro:roofDescriptor>
    </sbm_ro:roof>
    <buildingComponentsDoppelantentempel>
        <sbm_co:roundstairs>
            <sbm_co:numberOfSteps>4</sbm_co:numberOfSteps>
            <sbm_co:stepHeight>7</sbm_co:stepHeight>
            <sbm_co:stepWidth>150</sbm_co:stepWidth>
        </sbm_co:roundstairs>
        <sbm_co:fascia>
            <sbm_co:componentModelSource>
                <sbm_co:modelCategory>Moulding_Eckstein</sbm_co:modelCategory>
            </sbm_co:componentModelSource>
            <sbm_co:mouldingHeightRatio>0.6</sbm_co:mouldingHeightRatio>
            <sbm_co:targetFloors>
                <sbm_co:floorPosition>ALL</sbm_co:floorPosition>
            </sbm_co:targetFloors>
        </sbm_co:fascia>
        <sbm_co:pillar>
            <sbm_co:componentModelSource>
                <sbm_co:modelCategory>Pillar_Dorisch</sbm_co:modelCategory>
            </sbm_co:componentModelSource>
            
            <sbm_co:numberOfPillarsLongside>14</sbm_co:numberOfPillarsLongside>
            <sbm_co:numberOfPillarsBroadside>6</sbm_co:numberOfPillarsBroadside>
            
            <!--<sbm_co:pillarDistanceRatio>0.1</sbm_co:pillarDistanceRatio>-->
            <sbm_co:pillarFloorPosition>GROUND</sbm_co:pillarFloorPosition>
        </sbm_co:pillar>
        <sbm_co:edgeaddition>
            <sbm_co:componentModelSource>
                <sbm_co:modelCategory>Moulding_Eckstein</sbm_co:modelCategory>
            </sbm_co:componentModelSource>
            <sbm_co:edgeAdditionHeightRatio>0.05</sbm_co:edgeAdditionHeightRatio>
            <sbm_co:targetFloors>
                <sbm_co:floorPosition>ROOF</sbm_co:floorPosition>
            </sbm_co:targetFloors>
            <sbm_co:targetWalls>
                <sbm_co:direction>ROOF_SIDE</sbm_co:direction>
            </sbm_co:targetWalls>
        </sbm_co:edgeaddition>
    </buildingComponentsDoppelantentempel>
</buildingDoppelantentempel>
