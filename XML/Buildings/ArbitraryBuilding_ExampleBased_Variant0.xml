<?xml version="1.0" encoding="UTF-8"?>
<building xmlns="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Building"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sbm_fl="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Floor"
    xmlns:sbm_fp="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Footprint"
    xmlns:sbm_ct="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/CommonTypes"
    xmlns:sbm_ro="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Roof"
    xmlns:sbm_bu="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Building"
    xmlns:sbm_co="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Component"
    xmlns:sbm_cb="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/ClassBasedFootprint"
    xmlns:sbm_eb="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/ExampleBasedSynthesis"
    xmlns:sbm_op="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Objectplacement"
    xsi:schemaLocation="https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Building 
    https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/Schema/Building/BuildingSchema.xsd">
    <dimensions>
        <sbm_bu:width>
            <sbm_ct:lowerBorder>120.0</sbm_ct:lowerBorder>
            <sbm_ct:upperBorder>180.0</sbm_ct:upperBorder>
        </sbm_bu:width>
        <sbm_bu:height>
            <sbm_ct:lowerBorder>150.0</sbm_ct:lowerBorder>
            <sbm_ct:upperBorder>190.0</sbm_ct:upperBorder>
        </sbm_bu:height>
        <sbm_bu:length>
            <sbm_ct:lowerBorder>75.0</sbm_ct:lowerBorder>
            <sbm_ct:upperBorder>95.0</sbm_ct:upperBorder>
        </sbm_bu:length>
        <sbm_bu:wallThickness>3.0</sbm_bu:wallThickness>
    </dimensions>
    <sbm_ro:roof>
        <sbm_ct:extern>
            https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/XML/Roof/StandardRoof_EqualSlopes.xml
        </sbm_ct:extern>
    </sbm_ro:roof>
    <sbm_bu:floors>
        <sbm_fl:floor>
            <sbm_fl:floorposition>GROUND</sbm_fl:floorposition>
            <sbm_fp:footprint src="exampleBased">
                <sbm_ct:extern>
                    https://raw.github.com/pgunia/SemanticBuildingModeler_Ressources/master/XML/FootprintExampleBased/Footprint_ExampleBasedSynthesis.xml
                </sbm_ct:extern>
            </sbm_fp:footprint>
            <sbm_fl:floorHeight>
                <sbm_ct:lowerBorder>40.0</sbm_ct:lowerBorder>
                <sbm_ct:upperBorder>55.0</sbm_ct:upperBorder>
            </sbm_fl:floorHeight>
        </sbm_fl:floor>
        <sbm_fl:floor>
            <sbm_fl:floorposition>INTERMEDIATE</sbm_fl:floorposition>
            <sbm_fl:reuseFloorDefinition>GROUND</sbm_fl:reuseFloorDefinition>
            <sbm_fl:floorHeight>
                <sbm_ct:lowerBorder>55.0</sbm_ct:lowerBorder>
                <sbm_ct:upperBorder>75.0</sbm_ct:upperBorder>
            </sbm_fl:floorHeight>
        </sbm_fl:floor>
        <sbm_fl:floor>
            <sbm_fl:floorposition>TOP</sbm_fl:floorposition>
            <sbm_fl:reuseFloorDefinition>GROUND</sbm_fl:reuseFloorDefinition>
            <sbm_fl:floorHeight>
                <sbm_ct:lowerBorder>45.0</sbm_ct:lowerBorder>
                <sbm_ct:upperBorder>55.0</sbm_ct:upperBorder>
            </sbm_fl:floorHeight>
        </sbm_fl:floor>
    </sbm_bu:floors>
    
    <sbm_bu:buildingComponents>
        <sbm_co:component>
            <sbm_co:window>
                <sbm_co:componentModelSource>
                    <sbm_co:modelCategory>Window</sbm_co:modelCategory>
                </sbm_co:componentModelSource>
                <sbm_co:translationRatioForWindows>0.4</sbm_co:translationRatioForWindows>
                <sbm_co:minDistanceBetweenWindowsRatio>0.01</sbm_co:minDistanceBetweenWindowsRatio>
                <sbm_co:windowToTargetQuadRatio>
                    <sbm_ct:lowerBorder>0.3</sbm_ct:lowerBorder>
                    <sbm_ct:upperBorder>0.5</sbm_ct:upperBorder>
                </sbm_co:windowToTargetQuadRatio>
            </sbm_co:window>
        </sbm_co:component>
        <sbm_co:component>
            <sbm_co:door>
                <sbm_co:componentModelSource>
                    <sbm_co:modelCategory>Door</sbm_co:modelCategory>
                </sbm_co:componentModelSource>
                <sbm_co:distanceToCornerRatio>0.1</sbm_co:distanceToCornerRatio>
                <sbm_co:doorToTargetQuadRatio>
                    <sbm_ct:lowerBorder>0.5</sbm_ct:lowerBorder>
                </sbm_co:doorToTargetQuadRatio>
            </sbm_co:door>
        </sbm_co:component>
        <sbm_co:component>
            <sbm_co:moulding>
                <sbm_co:componentModelSource>
                    <sbm_co:modelCategory>Moulding_Eckstein</sbm_co:modelCategory>
                </sbm_co:componentModelSource>
                <sbm_co:verticalAlignment>TOP</sbm_co:verticalAlignment>
                <sbm_co:mouldingHeightRatio>0.05</sbm_co:mouldingHeightRatio>
                <sbm_co:targetFloors>
                    <sbm_co:floorPosition>ALL</sbm_co:floorPosition>
                </sbm_co:targetFloors>
                <sbm_co:targetWalls>
                    <sbm_co:direction>FRONT</sbm_co:direction>
                </sbm_co:targetWalls>
            </sbm_co:moulding>
        </sbm_co:component>
    </sbm_bu:buildingComponents>
</building>
